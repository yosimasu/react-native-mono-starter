{
  "presets": ["module:metro-react-native-babel-preset"],
  "env": {
    "development": {
      "plugins": [
        [
          "module-resolver", {
            "root": ["."],
            "alias": {
              "^@modules/(.+)": "./modules/\\1"
            }
          }
        ]
      ]
    }
  }
}
