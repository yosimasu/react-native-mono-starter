{
  "name": "react-native-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:clean": "cd android; ./gradlew clean",
    "clean": "yarn android:clean; yarn ios:clean",
    "ios": "react-native run-ios",
    "ios:clean": "cd ios; shx rm -rf build",
    "postinstall": "node ./scripts/link.js && rndebugger-open",
    "rename": "react-native-rename",
    "start": "react-native start --reset-cache --config ../../../../app/rn-cli.config.js",
    "test": "jest",
    "storybook": "storybook start --port 7007 --reset-cache --projectRoots ../"
  },
  "jest": {
    "preset": "react-native"
  },
  "dependencies": {
    "react": "16.3.1",
    "react-native": "0.55.4"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.4.11",
    "@storybook/addon-links": "3.4.11",
    "@storybook/addons": "3.4.11",
    "@storybook/react-native": "3.4.11",
    "babel-core": "6.26.3",
    "babel-preset-react-native-stage-0": "1.0.1",
    "babel-runtime": "6.26.0",
    "jest": "23.5.0",
    "jest-react-native": "18.0.0",
    "prop-types": "15.6.2",
    "react-dom": "16.3.1",
    "react-native-debugger-open": "0.3.17",
    "react-native-rename": "2.2.2",
    "react-test-renderer": "16.3.1"
  }
}
